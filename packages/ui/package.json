{
  "name": "@echo/ui",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./style.css": "./src/style.css",
    "./components/ui/*": {
      "types": "./dist/components/ui/*.d.ts",
      "default": "./dist/components/ui/*.js"
    },
    "./components/ui/GridPattern": {
      "types": "./dist/components/ui/GridPattern.d.ts",
      "default": "./dist/components/ui/GridPattern.js"
    },
    "./components/ui/card": {
      "types": "./dist/components/ui/card.d.ts",
      "default": "./dist/components/ui/card.js"
    },
    "./components/ui/separator": {
      "types": "./dist/components/ui/separator.d.ts",
      "default": "./dist/components/ui/separator.js"
    },
    "./components/ui/dialog": {
      "types": "./dist/components/ui/dialog.d.ts",
      "default": "./dist/components/ui/dialog.js"
    },
    "./components/ui/button": {
      "types": "./dist/components/ui/button.d.ts",
      "default": "./dist/components/ui/button.js"
    },
    "./icons/*": {
      "types": "./dist/icons/*.d.ts",
      "default": "./dist/icons/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "vaul": "^1.0.0",
    "@radix-ui/react-hover-card": "^1.1.2",
    "input-otp": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.11.17",
    "lucide-react": "^0.456.0",
    "react-hook-form": "^7.53.2",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "@echo/eslint-config": "file:../eslint-config",
    "@echo/tailwind-config": "file:../tailwind-config",
    "@echo/typescript-config": "file:../typescript-config",
    "@echo/utils": "file:../utils"
  },
  "devDependencies": {
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "react": "^18.3.1",
    "typescript": "^5.3.3"
  }
